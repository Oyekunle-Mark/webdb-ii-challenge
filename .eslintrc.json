{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "func-names": 0,
    "arrow-parens": 0,
    "space-before-function-paren": 0,
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "no-console": 0
  }
}
